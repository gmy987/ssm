<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.ssm.mapper.PointMapper">
    <insert id="insertPoint" parameterType="com.demo.ssm.po.Point">
        INSERT INTO node(node_id,lng,lat) VALUES(#{id},#{lng},#{lat});
    </insert>
</mapper>